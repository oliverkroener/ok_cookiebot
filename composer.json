{
    "name": "oliverkroener/ok-cookiebot",
    "type": "typo3-cms-extension",
    "description": "Provides a backend module to manage privacy cookie consent scripts for Cookiebot.",
    "license": "GPL-2.0-or-later",
    "version": "1.0.0",
    "keywords": [
        "typo3",
        "typo3-extension",
        "cookiebot",
        "cookie-consent",
        "gdpr",
        "privacy",
        "backend-module",
        "consent-management"
    ],
    "authors": [
        {
            "name": "Oliver Kroener",
            "email": "ok@oliver-kroener.de",
            "role": "developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^12.4 || ^13.4 || ^14.0",
        "typo3/cms-backend": "^12.4 || ^13.4 || ^14.0"
    },
    "autoload": {
        "psr-4": {
            "OliverKroener\\OkCookiebotCookieConsent\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ok_cookiebot"
        }
    }
}
